program {
    // Esto es un comentario de una línea.
    integer x = 5; // Declaración con comentario al lado.

    /*
       Esto es un comentario
       de múltiples líneas.
    */
    integer y = /* comentario anidado en expresión */ 10;

    integer WHILE = 15; // "WHILE" es un ID válido, no la palabra "while"

    void print_int(integer i) extern;
    
    void main() {
        print_int(x);  // Debe imprimir 5
        print_int(y);  // Debe imprimir 10
        print_int(WHILE); // Debe imprimir 15
    }
}